{
    "engines": {
        "engine-rn": {
            "id": "engine-rn",
            "overview": "Default react-native based engine. Runs standard react native apps with native support",
            "requiresMetroConfig": true,
            "paths": {
                "platformTemplatesDir": "$RNV_HOME/src/engine-rn/platformTemplates"
            },
            "plugins": {
                "react": "source:rnv",
                "react-art": "source:rnv",
                "react-dom": "source:rnv",
                "react-native": "source:rnv",
                "react-native-web": "source:rnv",
                "@react-native-community/cli-platform-ios": "source:rnv",
                "@react-native-community/cli": "source:rnv"
            },
            "npm": {
                "devDependencies": {
                    "@rnv/engine-rn": "0.31.3"
                }
            },
            "platforms": {
                "ios": {
                    "engine": "engine-rn",
                    "npm": {
                        "devDependencies": {
                            "xcode": "2.1.0"
                        },
                        "optionalDependencies": {
                            "ios-deploy": "1.10.0"
                        }
                    }
                },
                "android": {
                    "engine": "engine-rn",
                    "npm": {
                        "devDependencies": {
                            "jetifier": "1.6.5"
                        }
                    }
                },
                "androidwear": {
                    "npm": {
                        "devDependencies": {
                            "jetifier": "1.6.5"
                        }
                    }
                },
                "androidtv": {
                    "npm": {
                        "devDependencies": {
                            "jetifier": "1.6.5"
                        }
                    }
                },
                "tvos": {
                    "npm": {
                        "devDependencies": {
                            "xcode": "2.1.0"
                        },
                        "optionalDependencies": {
                            "ios-deploy": "1.10.0"
                        }
                    }
                }
            }
        },
        "engine-rn-electron": {
            "id": "engine-rn-electron",
            "overview": "React native based engine with web transpiler provided by react-native-web and native desktop wrapper provided by electron",
            "paths": {
                "platformTemplatesDir": "$RNV_HOME/src/engine-rn-electron/platformTemplates"
            },
            "plugins": {
                "react": "source:rnv",
                "react-art": "source:rnv",
                "react-dom": "source:rnv",
                "react-native": "source:rnv",
                "react-native-web": "source:rnv",
                "@react-native-community/cli-platform-ios": "source:rnv",
                "@react-native-community/cli": "source:rnv"
            },
            "npm": {
                "devDependencies": {
                    "@rnv/engine-rn-electron": "0.31.3"
                }
            },
            "platforms": {
                "macos": {
                    "npm": {
                        "devDependencies": {}
                    }
                },
                "windows": {
                    "npm": {
                        "devDependencies": {}
                    }
                }
            }
        },
        "engine-rn-next": {
            "id": "engine-rn-next",
            "overview": "React native based engine with web transpiler provided by react-native-web and render by nextjs",
            "paths": {
                "platformTemplatesDir": "$RNV_HOME/src/engine-rn-next/platformTemplates"
            },
            "plugins": {
                "react": "source:rnv",
                "react-art": "source:rnv",
                "react-dom": "source:rnv",
                "react-native": "source:rnv",
                "react-native-web": "source:rnv",
                "next": "source:rnv"
            },
            "npm": {
                "devDependencies": {
                    "@rnv/engine-rn-next": "0.31.3"
                }
            },
            "platforms": {
                "web": {}
            }
        },
        "engine-rn-web": {
            "id": "engine-rn-web",
            "overview": "React native based engine with web transpiler provided by react-native-web",
            "paths": {
                "platformTemplatesDir": "$RNV_HOME/src/engine-rn-web/platformTemplates"
            },
            "plugins": {
                "react": "source:rnv",
                "react-art": "source:rnv",
                "react-dom": "source:rnv",
                "react-native": "source:rnv",
                "react-native-web": "source:rnv",
                "@react-native-community/cli-platform-ios": "source:rnv",
                "@react-native-community/cli": "source:rnv"
            },
            "npm": {
                "devDependencies": {
                    "@rnv/engine-rn-web": "0.31.3"
                }
            },
            "platforms": {
                "tizen": {
                    "npm": {
                        "dependencies": {
                            "raf": "3.4.1"
                        }
                    }
                },
                "web": {},
                "webos": {},
                "tizenwatch": {},
                "tizenmobile": {},
                "chromecast": {},
                "kaios": {},
                "firefoxos": {},
                "firefoxtv": {}
            }
        }
    },
    "TODO": "This file will be migrated to separate engines in the future"
}
