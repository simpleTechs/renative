{
    "name": "@rnv/deploy-docker",
    "version": "0.3.0",
    "description": "Gives RNV the possibility to deploy web apps as a Docker image published to dockerhub",
    "main": "dist/index.js",
    "scripts": {
        "build": "babel src/index.js --out-dir dist",
        "build:clean": "rm -rf dist || true",
        "build:dev": "yarn build --retain-lines",
        "watch": "nodemon --watch src --exec yarn build:dev",
        "test": "echo \"Error: no test specified\" && exit 1",
        "prepublishOnly": "yarn build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/pavjacko/renative.git"
    },
    "keywords": [
        "rnv",
        "renative",
        "docker"
    ],
    "author": "Mihai Blaga <mihaiblaga89@gmail.com> (https://github.com/mihaiblaga89/)",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/pavjacko/renative/issues"
    },
    "publishConfig": {
        "access": "public"
    },
    "homepage": "https://github.com/pavjacko/renative#readme",
    "peerDependencies": {
        "rnv": "~0.31.2"
    },
    "devDependencies": {
        "@babel/cli": "7.8.3",
        "@babel/core": "7.8.3",
        "@babel/node": "7.8.3",
        "@babel/plugin-proposal-nullish-coalescing-operator": "7.8.3",
        "@babel/plugin-proposal-optional-chaining": "7.8.3",
        "@babel/preset-env": "7.8.7"
    },
    "dependencies": {
        "get-installed-path": "^4.0.8"
    }
}
